{
  "expo": {
    "name": "SustainX App",
    "slug": "trialll1",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "trialll1",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/SustainX.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "assets/fonts/*",
      "assets/images/*",
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.trialll1",
      "splash": {
        "image": "./assets/images/SustainX.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.trialll1",
      "splash": {
        "image": "./assets/images/SustainX.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/images/favicon.png",
      "bundler": "metro"
    },
    "newArchEnabled": true,
    "extra": {
      "eas": {
        "projectId": "9c716b01-3118-4110-be2e-2ea441b6b786"
      }
    },
    "plugins": [
      "expo-router"
    ]
  }
}
